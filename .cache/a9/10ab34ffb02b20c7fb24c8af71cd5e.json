al":{"line":53,"column":20}},{"generated":{"line":107,"column":18},"source":"components/ConductTransaction.js","original":{"line":52,"column":16}},{"generated":{"line":107,"column":19},"source":"components/ConductTransaction.js","original":{"line":33,"column":12}},{"generated":{"line":107,"column":20},"source":"components/ConductTransaction.js","original":{"line":32,"column":8}},{"generated":{"line":108,"column":0},"source":"components/ConductTransaction.js","original":{"line":62,"column":5}},{"name":"Component","generated":{"line":112,"column":2},"source":"components/ConductTransaction.js","original":{"line":5,"column":33}},{"generated":{"line":114,"column":0},"source":"components/ConductTransaction.js","original":{"line":63,"column":1}},{"name":"ConductTransaction","generated":{"line":115,"column":15},"source":"components/ConductTransaction.js","original":{"line":65,"column":15}}],"sources":{"components/ConductTransaction.js":"import React, { Component } from 'react';\nimport { FormGroup, FormControl,Button } from 'react-bootstrap';\nimport { Link } from 'react-router-dom';\n\nclass ConductTransaction extends Component {\n    state = { recipient: '', amount: 0};\n\n    updateRecipient = event => {\n        this.setState({ recipient : event.target.value});\n    }\n\n    updateAmount = event => {\n        this.setState({ amount : Number(event.target.value) });\n    }\n\n    conductTransaction = () => {\n        const { recipient, amount} = this.state;\n\n        fetch('http://localhost:3001/api/transact', {\n            method: 'POST',\n            headers: { 'Content-Type' : 'application/json' },\n            body: JSON.stringify({recipient,amount })\n    }).then(response => response.json())\n        .then(json => {\n            alert(json.message || json.type);\n        });    \n        \n    }\n\n    render() {\n\n        return (\n            <div className='ConductTransaction'>\n                <Link to = '/' className='button'> Home </Link>\n                <h3> Conduct a Transaction </h3>\n                <FormGroup>\n                    <FormControl \n                        input = 'text'\n                        placeholder = 'recipient'\n                        value={this.state.recipient}\n                        onChange={this.updateRecipient}    \n                    />\n                </FormGroup>\n                <FormGroup>\n                <FormControl \n                        input = 'number'\n                        placeholder = 'amount'\n                        value={this.state.amount}\n                        onChange={this.updateAmount}    \n                    />\n                </FormGroup>\n                <div>\n                    <Button \n                        bsstyle=\"danger\"\n                        onClick={this.conductTransaction}\n                    >\n                        Submit\n                    </Button>\n                </div>\n            </div>\n        )\n    }\n};\n\nexport default ConductTransaction;"},"lineCount":null}},"error":null,"hash":"c760507c410d587303922878408c2762","cacheData":{"env":{}}}