inal":{"line":67,"column":15}}],"sources":{"components/ConductTransaction.js":"import React, { Component } from 'react';\nimport { FormGroup, FormControl,Button } from 'react-bootstrap';\nimport Navigation from './Navigation';\n\nclass ConductTransaction extends Component {\n    state = { recipient: '', amount: 0};\n\n    updateRecipient = event => {\n        this.setState({ recipient : event.target.value});\n    }\n\n    updateAmount = event => {\n        this.setState({ amount : Number(event.target.value) });\n    }\n\n    conductTransaction = () => {\n        const { recipient, amount} = this.state;\n\n        fetch('http://localhost:3001/api/transact', {\n            method: 'POST',\n            headers: { 'Content-Type' : 'application/json' },\n            body: JSON.stringify({recipient,amount })\n    }).then(response => response.json())\n        .then(json => {\n            alert(json.message || json.type);\n        });    \n        \n    }\n\n    render() {\n\n        return (\n            <div>\n                <Navigation />\n                <div className='ConductTransaction'>\n                    <h3> Conduct a Transaction </h3>\n                    <FormGroup>\n                        <FormControl \n                            input = 'text'\n                            placeholder = 'recipient'\n                            value={this.state.recipient}\n                            onChange={this.updateRecipient}    \n                        />\n                    </FormGroup>\n                    <FormGroup>\n                    <FormControl \n                            input = 'number'\n                            placeholder = 'amount'\n                            value={this.state.amount}\n                            onChange={this.updateAmount}    \n                        />\n                    </FormGroup>\n                    <div>\n                        <Button \n                            bsstyle=\"danger\"\n                            onClick={this.conductTransaction}\n                        >\n                            Submit\n                        </Button>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n};\n\nexport default ConductTransaction;"},"lineCount":null}},"error":null,"hash":"112d9c8c21eca57634522b2989329bf5","cacheData":{"env":{}}}