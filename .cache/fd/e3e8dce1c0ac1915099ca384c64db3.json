    const stringifiedData = JSON.stringify(data);\n\n        const dataDisplay = \n            stringifiedData.length > 35 ?\n            '${stringifiedData.substring(0,35)...' :\n            stringifiedData;\n\n        if (this.state.displayTransaction){\n            return(\n                <div>\n                    {\n                        data.map(transaction => (\n                            <div key={transaction.id}>\n                                <hr />\n                                <Transaction transaction={transaction} />\n                            </div> \n                        ))\n                    }\n                    <br />\n                    <Button bsstyle=\"danger\" bssize=\"small\" onClick={this.toggleTransaction}>Show less: </Button>\n                </div>\n            )\n        }\n\n        return (    \n            <div>\n                <div>Data : {dataDisplay}</div>\n                <Button bsstyle=\"danger\" bssize=\"small\" onClick={this.toggleTransaction}>Show more: </Button>\n            </div>\n        );\n    }\n\n    render () {\n\n        const { timestamp, hash } =  this.props.block;\n        \n        const hashDisplay = '${hash.substring(0,15)}...';\n\n        return (\n            <div className='Block'>\n                <div> Hash : {hashDisplay} </div>\n                <div> Timestamp: {new Date(timestamp).toLocaleDateString()}</div>\n                {this.displayTransaction}\n            </div>\n            \n        );\n    }\n};\n\nexport default Block;"},"lineCount":null}},"error":null,"hash":"0f4453ff28f1ca486198c77bf8ea22e1","cacheData":{"env":{}}}